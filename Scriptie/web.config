    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.7.2" />
      </system.Web>
  -->
  <system.web>
    <httpHandlers>
      <add verb="*" path="*" type="Sysmo.Web.Api.ApiHttpHandler" />
    </httpHandlers>
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime maxRequestLength="10485760" enableVersionHeader="false" targetFramework="4.5.2" />
    <pages buffer="false" compilationMode="Never" enableSessionState="false" enableViewState="false" enableViewStateMac="false" validateRequest="false" />
    <sessionState mode="Off" />
  </system.web>
  <system.webServer>
    <!--
    <rewrite>
      <rules>
        <rule name="HTTP/S to HTTPS Redirect" enabled="true" stopProcessing="true">
          <match url="(.*)" />
          <conditions logicalGrouping="MatchAny">
            <add input="{HTTPS}" pattern="^OFF$" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}{REQUEST_URI}" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>-->
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="1073741824" />
      </requestFiltering>
    </security>
    <applicationInitialization skipManagedModules="true" />
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="false">
      <remove name="WebDAVModule" />
    </modules>
    <handlers>
      <clear />
      <add name="Favicon Ico" path="favicon.ico" verb="*" modules="StaticFileModule,DefaultDocumentModule" resourceType="File" />
      <add name="Favicon Png" path="*.png" verb="*" modules="StaticFileModule,DefaultDocumentModule" resourceType="File" />
      <add name="Browser Config" path="browserconfig.xml" verb="*" modules="StaticFileModule,DefaultDocumentModule" resourceType="File" />
      <add name="Manifest Json" path="manifest.json" verb="*" modules="StaticFileModule,DefaultDocumentModule" resourceType="File" />
      <add name="SysmoWebApi-ExtensionlessUrlHandler" verb="*" path="*" type="Sysmo.Web.Api.ApiHttpHandler" />
    </handlers>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
        <add name="X-UA-Compatible" value="IE=edge,chrome=1" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
